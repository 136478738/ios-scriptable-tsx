  "main": "index.js",
  "scripts": {
    "watch": "set watching=true&& nodemon --watch src/ --ext ts,tsx,json,js --ignore 'src/**/*.spec.ts' --ignore 'src/output/**' --exec \"node -e console.clear()&& npm run dev\"",
    "dev": "set NODE_ENV=development&& set compileType=one&& ts-node ./src/compile.ts",
    "dev:all": "set NODE_ENV=development&& set compileType=all&& ts-node ./src/compile.ts",
    "build": "set NODE_ENV=production&& set compileType=one&& ts-node ./src/compile.ts",
    "build:all": "set NODE_ENV=production&& set compileType=all&& ts-node ./src/compile.ts"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.2",
    "@types/express": "^4.17.9",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.6.0",
    "@types/prettier": "^2.1.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/rimraf": "^3.0.0",
    "@types/scriptable-ios": "^1.6.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "esbuild": "^0.8.12",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "javascript-obfuscator": "^2.9.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "lodash": "^4.17.20",
    "qrcode-terminal": "^0.12.0",
    "rimraf": "^3.0.2"
  }
